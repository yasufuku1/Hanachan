<div class="main">
  <h2><span>通知一覧</span></h2>
  <% notifications = @notifications.where.not(visitor_id: current_user.id) %>
  <div class="notification-box">
　<% if notifications.exists? %>
    <%= render "notification", notifications: notifications %>
　<% else %>
　  <p>通知はありません</p>
　<% end %>
　</div>
</div>