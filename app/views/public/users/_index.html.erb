<% users.each do |user| %>
<div class="user-sec01">
  <div class="user-item">
  <%= link_to user_path(user) do %>
    <%= image_tag user.get_profile_image(80, 80), class: "profile-img" %>
  </div>
  <div class="user-name-item">
    <%= user.name %>
  <% end %>
  </div>
  <div class="user-item">
    <% if current_user.name != "guestuser" && current_user.id != user.id %>
      <%= render "public/relationships/relationships_btn", user: user %>
    <% end %>
  </div>
</div>
<% end %>
